{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.6.0" } }],
          "react",
          "stage-0"
        ],
      "plugins": [
          "transform-async-to-generator",
          "transform-class-properties",
          "transform-es2015-arrow-functions",
          "transform-es2015-classes",
          "react-hot-loader/babel",
          "dynamic-import-node"
      ],
      "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.6.0" } }],
          "react",
          "stage-0"
        ],
      "plugins": [
          "transform-async-to-generator",
          "transform-class-properties",
          "transform-es2015-arrow-functions",
          "transform-es2015-classes",
          "react-hot-loader/babel",
          "dynamic-import-node"
        ],
      "sourceMaps": "none"
      }
    }
  }
}